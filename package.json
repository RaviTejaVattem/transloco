{
  "name": "transloco-playground",
  "version": "0.0.0",
  "scripts": {
    "cypress-open": "cypress open",
    "cypress-run": "cypress run",
    "ng": "ng",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors init",
    "hooks:pre-commit": "node hooks/pre-commit.js",
    "release": "standard-version",
    "commit": "git-cz",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "test:lib": "ng test transloco",
    "test:schematics": "npm test --prefix ./schematics",
    "test:headless": "cross-env CI=true ng test",
    "test:covergae": "npm run test:headless -- --code-coverage",
    "schematics:watch": "cd ./schematics/ && npm link && cd .. && npm link schematics && npm run build --prefix ./schematics -- --watch",
    "lint": "ng lint",
    "e2e": "cypress open",
    "e2e:ci": "cypress run"
  },
  "dependencies": {
    "@angular/animations": "~8.1.1",
    "@angular/common": "~8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "~8.1.1",
    "@angular/forms": "~8.1.1",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/router": "~8.1.1",
    "@briebug/cypress-schematic": "^2.0.0",
    "@schuchard/prettier": "^3.1.0",
    "commitizen": "^4.0.3",
    "rxjs": "~6.4.0",
    "standard-version": "^6.0.1",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular-devkit/build-ng-packagr": "~0.801.1",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "~8.1.1",
    "@angular/language-service": "~8.1.1",
    "@commitlint/cli": "8.1.0",
    "@commitlint/config-angular": "8.1.0",
    "@commitlint/config-conventional": "8.1.0",
    "@cypress/webpack-preprocessor": "4.1.0",
    "@netbasal/spectator": "^3.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "all-contributors-cli": "^6.8.1",
    "chalk": "^2.4.2",
    "codelyzer": "^5.0.0",
    "copy": "^0.3.2",
    "cross-env": "^5.2.0",
    "cypress": "3.4.0",
    "husky": "^3.0.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lint-staged": "9.2.0",
    "ng-mocks": "^8.1.0",
    "ng-packagr": "^5.1.0",
    "prettier": "1.18.2",
    "protractor": "~5.4.0",
    "ts-loader": "6.0.4",
    "ts-mocks": "^2.3.4",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~3.4.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "npm run hooks:pre-commit && lint-staged",
      "pre-push": "npm run test:headless"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,component.html}": [
      "prettier --write",
      "git add"
    ]
  }
}
